<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidbootcamp.coffeefinderapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MochasJavasActivity" android:label="Mochas &amp; Javas" />
        <activity android:name=".JosCafeActivity" android:label="Jo\'s Cafe" />
        <activity android:name=".CoffeeBarActivity" android:label="The Coffee Bar" />
        <activity android:name=".StellarActivity" android:label="stellar coffee co." />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>